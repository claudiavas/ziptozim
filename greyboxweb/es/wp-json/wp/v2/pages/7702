b{}\u00bb][\/et_pb_divider][et_pb_code admin_label=\u00bbCode to equalize height of blog grid\u00bb _builder_version=\u00bb4.16&#8243; _module_preset=\u00bbdefault\u00bb global_colors_info=\u00bb{}\u00bb]<script><!-- [et_pb_line_break_holder] -->(function($) {<!-- [et_pb_line_break_holder] -->    $(document).ready(function() {<!-- [et_pb_line_break_holder] -->        $(window).resize(function() {<!-- [et_pb_line_break_holder] -->            $('.et_blog_grid_equal_height').each(function() {<!-- [et_pb_line_break_holder] -->                equalise_articles($(this));<!-- [et_pb_line_break_holder] -->            });<!-- [et_pb_line_break_holder] -->        });<!-- [et_pb_line_break_holder] --><!-- [et_pb_line_break_holder] -->        $('.et_blog_grid_equal_height').each(function() {<!-- [et_pb_line_break_holder] -->            var blog = $(this);<!-- [et_pb_line_break_holder] --><!-- [et_pb_line_break_holder] -->            equalise_articles($(this));<!-- [et_pb_line_break_holder] --><!-- [et_pb_line_break_holder] -->            var observer = new MutationObserver(function(mutations) {<!-- [et_pb_line_break_holder] -->                equalise_articles(blog);<!-- [et_pb_line_break_holder] -->            });<!-- [et_pb_line_break_holder] -->            <!-- [et_pb_line_break_holder] -->            var config = {<!-- [et_pb_line_break_holder] -->                subtree: true,<!-- [et_pb_line_break_holder] -->                childList: true <!-- [et_pb_line_break_holder] -->            };<!-- [et_pb_line_break_holder] --><!-- [et_pb_line_break_holder] -->            observer.observe(blog[0], config);<!-- [et_pb_line_break_holder] -->        });<!-- [et_pb_line_break_holder] --><!-- [et_pb_line_break_holder] -->        function equalise_articles(blog) {<!-- [et_pb_line_break_holder] -->            var articles = blog.find('article');<!-- [et_pb_line_break_holder] -->            var heights = [];<!-- [et_pb_line_break_holder] -->            <!-- [et_pb_line_break_holder] -->            articles.each(function() {<!-- [et_pb_line_break_holder] -->                var height = 0;<!-- [et_pb_line_break_holder] -->                height += ($(this).find('.et_pb_image_container, .et_main_video_container').length != 0) ? $(this).find('.et_pb_image_container, .et_main_video_container').outerHeight(true) : 0;<!-- [et_pb_line_break_holder] -->                height += $(this).find('.entry-title').outerHeight(true);<!-- [et_pb_line_break_holder] -->                height += ($(this).find('.post-meta').length != 0) ? $(this).find('.post-meta').outerHeight(true) : 0; <!-- [et_pb_line_break_holder] -->                height += ($(this).find('.post-content').length != 0) ? $(this).find('.post-content').outerHeight(true) : 0;    <!-- [et_pb_line_break_holder] --><!-- [et_pb_line_break_holder] -->                heights.push(height);<!-- [et_pb_line_break_holder] -->            });<!-- [et_pb_line_break_holder] --><!-- [et_pb_line_break_holder] -->            var max_height = Math.max.apply(Math,heights); <!-- [et_pb_line_break_holder] --><!-- [et_pb_line_break_holder] -->            articles.each(function() {<!-- [et_pb_line_break_holder] -->                $(this).height(max_height);<!-- [et_pb_line_break_holder] -->            });<!-- [et_pb_line_break_holder] -->        }<!-- [et_pb_line_break_holder] --><!-- [et_pb_line_break_holder] -->        $(document).ajaxComplete(function() {<!-- [et_pb_line_break_holder] -->            $('.et_blog_grid_equal_height').imagesLoaded().then(function() {<!-- [et_pb_line_break_holder] -->                $('.et_blog_grid_equal_height').each(function(){<!-- [et_pb_line_break_holder] -->                    equalise_articles($(this));<!-- [et_pb_line_break_holder] -->                });<!-- [et_pb_line_break_holder] -->            });<!-- [et_pb_line_break_holder] -->        });<!-- [et_pb_line_break_holder] --><!-- [et_pb_line_break_holder] -->        $.fn.imagesLoaded = function() {<!-- [et_pb_line_break_holder] -->            var $imgs = this.find('img[src!=\"\"]');<!-- [et_pb_line_break_holder] -->            var dfds = [];<!-- [et_pb_line_break_holder] --><!-- [et_pb_line_break_holder] -->            if (!$imgs.length) {<!-- [et_pb_line_break_holder] -->                return $.Deferred().resolve().promise();<!-- [et_pb_line_break_holder] -->            }            <!-- [et_pb_line_break_holder] --><!-- [et_pb_line_break_holder] -->            $imgs.each(function(){<!-- [et_pb_line_break_holder] -->                var dfd = $.Deferred();<!-- [et_pb_line_break_holder] -->                dfds.push(dfd);<!-- [et_pb_line_break_holder] -->                var img = new Image();<!-- [et_pb_line_break_holder] --><!-- [et_pb_line_break_holder] -->                img.onload = function() {<!-- [et_pb_line_break_holder] -->                    dfd.resolve();<!-- [et_pb_line_break_holder] -->                };<!-- [et_pb_line_break_holder] --><!-- [et_pb_line_break_holder] -->                img.onerror = function() {<!-- [et_pb_line_break_holder] -->                    dfd.resolve(); <!-- [et_pb_line_break_holder] -->                };<!-- [et_pb_line_break_holder] --><!-- [et_pb_line_break_holder] -->                img.src = this.src;<!-- [et_pb_line_break_holder] -->            });<!-- [et_pb_line_break_holder] --><!-- [et_pb_line_break_holder] -->            return $.when.apply($, dfds);<!-- [et_pb_line_break_holder] -->        }<!-- [et_pb_line_break_holder] -->    });<!-- [et_pb_line_break_holder] -->})(jQuery);<!-- [et_pb_line_break_holder] --><\/script>[\/et_pb_code][\/et_pb_column][\/et_pb_row][et_pb_row use_custom_gutter=\u00bbon\u00bb gutter_width=\u00bb2&#8243; make_equal=\u00bbon\u00bb disabled_on=\u00bboff|off|off\u00bb _builder_version=\u00bb4.16&#8243; _module_preset=\u00bbdefault\u00bb global_colors_info=\u00bb{}\u00bb][et_pb_column type=\u00bb4_4&#8243; _builder_version=\u00bb4.16&#8243; _module_preset=\u00bbdefault\u00bb global_colors_info=\u00bb{}\u00bb][et_pb_text 