_holder] -->            var heights = [];<!-- [et_pb_line_break_holder] -->            <!-- [et_pb_line_break_holder] -->            articles.each(function() {<!-- [et_pb_line_break_holder] -->                var height = 0;<!-- [et_pb_line_break_holder] -->                height += ($(this).find('.et_pb_image_container, .et_main_video_container').length != 0) ? $(this).find('.et_pb_image_container, .et_main_video_container').outerHeight(true) : 0;<!-- [et_pb_line_break_holder] -->                height += $(this).find('.entry-title').outerHeight(true);<!-- [et_pb_line_break_holder] -->                height += ($(this).find('.post-meta').length != 0) ? $(this).find('.post-meta').outerHeight(true) : 0; <!-- [et_pb_line_break_holder] -->                height += ($(this).find('.post-content').length != 0) ? $(this).find('.post-content').outerHeight(true) : 0;    <!-- [et_pb_line_break_holder] --><!-- [et_pb_line_break_holder] -->                heights.push(height);<!-- [et_pb_line_break_holder] -->            });<!-- [et_pb_line_break_holder] --><!-- [et_pb_line_break_holder] -->            var max_height = Math.max.apply(Math,heights); <!-- [et_pb_line_break_holder] --><!-- [et_pb_line_break_holder] -->            articles.each(function() {<!-- [et_pb_line_break_holder] -->                $(this).height(max_height);<!-- [et_pb_line_break_holder] -->            });<!-- [et_pb_line_break_holder] -->        }<!-- [et_pb_line_break_holder] --><!-- [et_pb_line_break_holder] -->        $(document).ajaxComplete(function() {<!-- [et_pb_line_break_holder] -->            $('.et_blog_grid_equal_height').imagesLoaded().then(function() {<!-- [et_pb_line_break_holder] -->                $('.et_blog_grid_equal_height').each(function(){<!-- [et_pb_line_break_holder] -->                    equalise_articles($(this));<!-- [et_pb_line_break_holder] -->                });<!-- [et_pb_line_break_holder] -->            });<!-- [et_pb_line_break_holder] -->        });<!-- [et_pb_line_break_holder] --><!-- [et_pb_line_break_holder] -->        $.fn.imagesLoaded = function() {<!-- [et_pb_line_break_holder] -->            var $imgs = this.find('img[src!=\"\"]');<!-- [et_pb_line_break_holder] -->            var dfds = [];<!-- [et_pb_line_break_holder] --><!-- [et_pb_line_break_holder] -->            if (!$imgs.length) {<!-- [et_pb_line_break_holder] -->                return $.Deferred().resolve().promise();<!-- [et_pb_line_break_holder] -->            }            <!-- [et_pb_line_break_holder] --><!-- [et_pb_line_break_holder] -->            $imgs.each(function(){<!-- [et_pb_line_break_holder] -->                var dfd = $.Deferred();<!-- [et_pb_line_break_holder] -->                dfds.push(dfd);<!-- [et_pb_line_break_holder] -->                var img = new Image();<!-- [et_pb_line_break_holder] --><!-- [et_pb_line_break_holder] -->                img.onload = function() {<!-- [et_pb_line_break_holder] -->                    dfd.resolve();<!-- [et_pb_line_break_holder] -->                };<!-- [et_pb_line_break_holder] --><!-- [et_pb_line_break_holder] -->                img.onerror = function() {<!-- [et_pb_line_break_holder] -->                    dfd.resolve(); <!-- [et_pb_line_break_holder] -->                };<!-- [et_pb_line_break_holder] --><!-- [et_pb_line_break_holder] -->                img.src = this.src;<!-- [et_pb_line_break_holder] -->            });<!-- [et_pb_line_break_holder] --><!-- [et_pb_line_break_holder] -->            return $.when.apply($, dfds);<!-- [et_pb_line_break_holder] -->        }<!-- [et_pb_line_break_holder] -->    });<!-- [et_pb_line_break_holder] -->})(jQuery);<!-- [et_pb_line_break_holder] --><\/script>[\/et_pb_code][\/et_pb_column][\/et_pb_row][et_pb_row use_custom_gutter=&#8221;on&#8221; gutter_width=&#8221;2&#8243; make_equal=&#8221;on&#8221; disabled_on=&#8221;off|off|off&#8221; _builder_version=&#8221;4.16&#8243; _module_preset=&#8221;default&#8221; global_colors_info=&#8221;{}&#8221;][et_pb_column type=